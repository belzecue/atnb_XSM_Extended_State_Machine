;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "State",
"language": "GDScript",
"path": "res://addons/xsm/state.gd"
}, {
"base": "State",
"class": "StateAnimation",
"language": "GDScript",
"path": "res://addons/xsm/state_animation.gd"
}, {
"base": "State",
"class": "StateLoop",
"language": "GDScript",
"path": "res://addons/xsm/state_loop.gd"
}, {
"base": "State",
"class": "StateRand",
"language": "GDScript",
"path": "res://addons/xsm/state_rand.gd"
}, {
"base": "State",
"class": "StateRegions",
"language": "GDScript",
"path": "res://addons/xsm/state_regions.gd"
} ]
_global_script_class_icons={
"State": "res://addons/xsm/icons/state.png",
"StateAnimation": "res://addons/xsm/icons/state_animation.png",
"StateLoop": "res://addons/xsm/icons/state_loop.png",
"StateRand": "res://addons/xsm/icons/state_rand.png",
"StateRegions": "res://addons/xsm/icons/state_regions.png"
}

[application]

config/name="XSM - Extended State Machine"
run/main_scene="res://examples/platformer/platformer_example.tscn"
config/icon="res://icon.png"

[display]

window/size/width=512
window/size/height=300
window/stretch/mode="viewport"
window/stretch/aspect="keep"

[editor]

script_templates_search_path="res://addons/xsm/templates/"

[editor_plugins]

enabled=PoolStringArray( "res://addons/xsm/plugin.cfg" )

[gdnative]

singletons=[  ]

[importer_defaults]

texture={
"compress/bptc_ldr": 0,
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 0,
"compress/normal_map": 0,
"detect_3d": true,
"flags/anisotropic": false,
"flags/filter": false,
"flags/mipmaps": false,
"flags/repeat": 0,
"flags/srgb": 2,
"process/HDR_as_SRGB": false,
"process/fix_alpha_border": true,
"process/invert_color": false,
"process/premult_alpha": false,
"size_limit": 0,
"stream": false,
"svg/scale": 1.0
}

